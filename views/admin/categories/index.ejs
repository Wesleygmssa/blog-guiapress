<%- include('../../components/header.ejs'); %>
    <%- include('../../components/navbar.ejs'); %>


        <div class="container">
            <hr>
            <h2>Categoria</h2>
            <hr>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Título</th>
                        <th>Slug</th>
                        <th>Ações</th>
                    </tr>
                </thead>
                <tbody>
                    <% categories.forEach(category=>{ %>
                        <tr>
                            <th>
                                <%= category.id %>
                            </th>
                            <th>
                                <%= category.title %>
                            </th>
                            <th>
                                <%= category.slug %>
                            </th>
                            <th></th>

                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>


        <%- include('../../components/footer.ejs'); %>